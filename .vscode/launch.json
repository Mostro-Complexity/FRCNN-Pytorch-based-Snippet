    "version": "0.2.0",
    "configurations": [
        {
            "name": "聚类产生intra classes（VOC数据集）",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/intra_classes_clustering.py",
            "console": "integratedTerminal",
            "args": [
                "-d=data/VOC",
                "--dataset_name=voc2007"
            ]
        },
        {
            "name": "推断（使用聚类标签）",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/infer.py",
            "console": "integratedTerminal",
            "args": [
                "--image_min_side=400",
                "--image_max_side=600",
                "--backbone_path=backbone/resnet50-19c8e357.pth",
                "--model_path=checkpoint/checkpoint-intra-class.pth",
                "--input_dir=data/COCO2017/test2017",
                "--output_dir=outputs"
            ]
        },
        {
            "name": "训练（使用COCO聚类标签）",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/train.py",
            "console": "integratedTerminal",
            "args": [
                "--image_min_side=400",
                "--image_max_side=600",
                // "--current_epoch=17",
                "--learning_rate=0.001",
                "--workers=4",
                "--epochs=10",
                "--num_steps_to_display=500",
                "-b=backbone/resnet50-19c8e357.pth",
                "-d=data/COCO2017",
                "--intra_class"
            ]
        },
        {
            "name": "训练（使用VOC聚类标签）",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/train.py",
            "console": "integratedTerminal",
            "args": [
                "--image_min_side=400",
                "--image_max_side=600",
                // "--current_epoch=17",
                "--learning_rate=0.001",
                "--workers=4",
                "--epochs=100",
                "--num_steps_to_display=500",
                "-b=backbone/resnet50-19c8e357.pth",
                "-d=data/VOC",
                "--dataset_name=voc2007",
                "--intra_class"
            ]
        },
        {
            "name": "训练（使用原始标签）",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/train.py",
            "console": "integratedTerminal",
            "args": [
                "--image_min_side=400",
                "--image_max_side=600",
                "--learning_rate=0.001",
                "--workers=4",
                "--epochs=10",
                "--num_steps_to_display=500",
                "-b=backbone/resnet50-19c8e357.pth",
                "-d=data/COCO2017"
            ]
        },
        {
            "name": "评估（使用COCO聚类标签）",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/eval.py",
            "console": "integratedTerminal",
            "args": [
                "--workers=8",
                "--image_min_side=400",
                "--image_max_side=600",
                "--backbone_path=backbone/resnet50-19c8e357.pth",
                "--model_path=checkpoint/checkpoint-epoch0009.pth",
                "-d=data/COCO2017",
                "--intra_class"
            ]
        },
        {
            "name": "评估（使用VOC聚类标签）",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/eval.py",
            "console": "integratedTerminal",
            "args": [
                "--workers=8",
                "--image_min_side=400",
                "--image_max_side=600",
                "--backbone_path=backbone/resnet50-19c8e357.pth",
                "--model_path=checkpoint/checkpoint-intra-classes.pth",
                "-d=data/VOC",
                "--dataset_name=voc2007",
                "--intra_class"
            ]
        },
        {
            "name": "评估（使用COCO原始标签）",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/eval.py",
            "console": "integratedTerminal",
            "args": [
                "--workers=8",
                "--image_min_side=400",
                "--image_max_side=600",
                "--backbone_path=backbone/resnet50-19c8e357.pth",
                "--model_path=checkpoint/checkpoint-original-class.pth",
                "-d=data/COCO2017"
            ]
        }
    ]
}